<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="600.0" spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.BlastWindow.BlastController">
    <children>
        <HBox spacing="5.0">
            <children>
                <MenuButton mnemonicParsing="false" text="Database">
                    <items>
                        <RadioMenuItem fx:id="nrDatabase" mnemonicParsing="false" text="nr" />
                        <RadioMenuItem fx:id="database1" mnemonicParsing="false" text="Action 1" />
                        <RadioMenuItem fx:id="database2" mnemonicParsing="false" text="Action 2" />
                        <RadioMenuItem fx:id="database3" mnemonicParsing="false" text="Action 3" />
                        <RadioMenuItem fx:id="database4" mnemonicParsing="false" text="Action 4" />
                    </items>
                </MenuButton>
                <MenuButton mnemonicParsing="false" text="Program">
                    <items>
                        <RadioMenuItem fx:id="blastxProgram" mnemonicParsing="false" text="blastx" />
                        <RadioMenuItem fx:id="megablastProgram" mnemonicParsing="false" text="megablast" />
                        <RadioMenuItem fx:id="blastnProgram" mnemonicParsing="false" text="blastn" />
                        <RadioMenuItem fx:id="blastpProgram" mnemonicParsing="false" text="blastp" />
                        <RadioMenuItem fx:id="rpsblastProgram" mnemonicParsing="false" text="rpsblast" />
                    </items>
                </MenuButton>
            </children>
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </HBox>
      <TextArea fx:id="textArea" editable="false" prefHeight="200.0" promptText="Select database and program, and then press Run" wrapText="true" />
        <HBox spacing="5.0">
            <children>
                <ProgressBar fx:id="progressBar" prefWidth="225.0" progress="0.0" visible="false" />
                <Button fx:id="runButton" mnemonicParsing="false" text="Run" />
                <Button fx:id="resetButton" mnemonicParsing="false" text="Reset" />
                <Button fx:id="closeButton" mnemonicParsing="false" text="Close" />
            </children>
        </HBox>
    </children>
   <padding>
      <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
   </padding>
</VBox>
